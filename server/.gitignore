function slayDragon(k,l,m,n,d) {
    
    let count = 0;
    let j = 0;
    
    for(i=0; i < d.length; i++){
        
        if(j[i] == k){
            count +=1
            j+=1
    }else if(j[i] == l){
        count+=1
        j+=1
    }else if(j[i] == m){
        count +=1;
        j+=1;
    }else if(j[i] == n){
        count +=1;
        
    }
}